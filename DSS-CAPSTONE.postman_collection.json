									"            <th>Movie Year</th>\r",
									"            <th>Movie Review</th>\r",
									"            <th>Posted Date</th>\r",
									"            <th>Rating</th>\r",
									"        </tr>\r",
									"        {{#each response}}\r",
									"            <tr>\r",
									"                <td>{{movie.movieTitle}}</td>\r",
									"                <td>{{movie.movieYear}}</td>\r",
									"                <td>{{description}}</td>\r",
									"                <td>{{datePosted}}</td>\r",
									"                <td>{{rating}}</td>\r",
									"                <td>{{subsystem}}</td>\r",
									"                <td>{{message}}</td>\r",
									"            </tr>\r",
									"        {{/each}}\r",
									"    </table>\r",
									"`;\r",
									"pm.visualizer.set(template, {\r",
